{
    "[markdown]": {},
    "[python]": {
        "editor.formatOnType": true
    },
    "extensions.ignoreRecommendations": false,
    "[proto3]": {
        "editor.tabSize": 2
    },
    "[terraform]": {
        "editor.tabSize": 2
    },
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.folders.theme": "classic",
    "workbench.colorCustomizations": {
        "statusBar.background": "#2A2B2A",
        "statusBar.foreground": "#d4d4d4",
        "statusBarItem.hoverBackground": "#add6ff26",
        "statusBar.noFolderBackground": "#2A2B2A",
        "statusBar.debuggingBackground": "#2A2B2A",
        "editor.selectionBackground": "#474747",
        "sideBar.background": "#363636",
        "editor.background": "#2A2B2A",
        "list.activeSelectionBackground": "#474747",
        "list.inactiveSelectionBackground": "#474747",
    },
    "editor.links": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "string",
                "settings": {
                    "foreground": "#8CA67D"
                }
            },
            {
                "scope": "string.tag",
                "settings": {
                    "foreground": "#8CA67D"
                }
            },
            {
                "scope": "string.value",
                "settings": {
                    "foreground": "#8CA67D"
                }
            },
            {
                "scope": "string.regexp",
                "settings": {
                    "foreground": "#6A8759"
                }
            },
            {
                "scope": [
                    "keyword.operator",
                    "entity.name.type.go"
                ],
                "settings": {
                    "foreground": "#b1c3d7"
                }
            },
            {
               "scope": [
                    "constant",
                    "constant.numeric",
                    "constant.character",
                    "constant.other",
                    "variable.other.constant",
                    "meta.preprocessor.macro"
                ],
                "settings": {
                    "foreground": "#CECCAF"
                }
            },
            {
                "scope": [
                    "variable",
                    "variable.parameter",
                    "variable.other.property"
                ],
                "settings": {
                    "foreground": "#c7c6c6"
                }
            },
        ]
    },
    "git.enableSmartCommit": true,
    "editor.suggest.localityBonus": true,
    "editor.multiCursorModifier": "alt",
    "editor.suggestSelection": "first",
    "files.exclude": {
        ".idea": true,
        ".vscode": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true
    },
    "go.useLanguageServer": true,
    "go.formatTool": "goimports",
    "go.formatFlags": ["-local", "github.com/tetrateio/tetrate"],
    "go.toolsManagement.autoUpdate": true,
    "editor.largeFileOptimizations": false,
    "terraform.languageServer.enable": true,
    "git.openRepositoryInParentFolders": "never",
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    "breadcrumbs.enabled": false,
    "editor.fontSize": 14,
    "workbench.editor.empty.hint": "hidden",
    "highlight-bad-chars.additionalUnicodeChars": [
        "\u0000",
        "\u001f"
    ],
    "editor.minimap.enabled": false,
    "github.copilot.nextEditSuggestions.enabled": true,
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "editor.language.colorizedBracketPairs": [],
    "workbench.colorTheme": "Darcula Theme from IntelliJ",
    "go.alternateTools": {
        "dlv": "/opt/homebrew/bin/dlv"
    },
    "redhat.telemetry.enabled": false,
    "window.nativeTabs": true,
    "window.systemColorTheme": "auto",
    "docker.extension.enableComposeLanguageServer": false
}
