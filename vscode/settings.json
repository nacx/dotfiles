{
    "[markdown]": {},
    "[python]": {},
    "extensions.ignoreRecommendations": false,
    "workbench.colorTheme": "Darcula Theme from IntelliJ",
    "[proto3]": {
        "editor.tabSize": 2
    },
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.folders.theme": "classic",
    "workbench.colorCustomizations": {
        "statusBar.background": "#2A2B2A",
        "statusBar.foreground": "#d4d4d4",
        "statusBarItem.hoverBackground": "#add6ff26",
        "statusBar.noFolderBackground": "#2A2B2A",
        "statusBar.debuggingBackground": "#2A2B2A",
        "editor.selectionBackground": "#474747",
        "sideBar.background": "#363636",
        "editor.background": "#2A2B2A",
        "list.activeSelectionBackground": "#474747",
        "list.inactiveSelectionBackground": "#474747"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "string",
                "settings": {
                    "foreground": "#8CA67D"
                }
            },
            {
                "scope": "string.tag",
                "settings": {
                    "foreground": "#8CA67D"
                }
            },
            {
                "scope": "string.value",
                "settings": {
                    "foreground": "#8CA67D"
                }
            },
            {
                "scope": "string.regexp",
                "settings": {
                    "foreground": "#6A8759"
                }
            },
            {
                "scope": "keyword.operator",
                "settings": {
                    "foreground": "#93A5BA"
                }
            },
            {
                "name": "User-defined constant",
                "scope": [
                    "constant.character",
                    "constant.other"
                ],
                "settings": {
                    "foreground": "#CECCAF"
                }
            },
            {
                "scope": [
                    "variable"
                ],
                "settings": {
                    "foreground": "#e9adbf"
                }
            }
        ]
    },
    "git.enableSmartCommit": true,
    "window.zoomLevel": 0,
    "breadcrumbs.enabled": true,
    "editor.suggest.localityBonus": true,
    "editor.multiCursorModifier": "alt",
    "java.home": "/Library/Java/JavaVirtualMachines/jdk1.8.0_181.jdk/Contents/Home",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        ".idea": true,
        ".vscode": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true
    },
    "go.useLanguageServer": true,
    "go.goroot": "/usr/local/opt/go/libexec",
    "go.autocompleteUnimportedPackages": true,
    "go.formatTool": "goimports",
    "go.formatFlags": ["-local", "github.com/tetrateio/tetrate"],
    "go.liveErrors": {
        "enabled": false,
        "delay": 500
    },
    "terraform.indexing": {
        "enabled": false,
        "liveIndexing": false,
        "delay": 500,
        "exclude": [
            ".terraform/**/*",
            "**/.terraform/**/*"
        ]
    },
    "terraform.languageServer": {
        "external": true
    }
}
